class Solution {
public:
    vector<int> sortArray(vector<int>& nums) {
        // int n= nums.size();
        // for(int i= 0;i<n;i++){
        //     for(int j=1;j<n-1;j++){
        //         if(nums[i]>nums[j]){
        //             int temp = nums[i];
        //             nums[i]=nums[j];
        //             nums[j]=temp;
        //         }
        //     }
        // }
        sort(nums.begin(),nums.end());
        return nums;
    }
};
